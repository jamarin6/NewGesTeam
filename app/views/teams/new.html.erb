
<h1 class="center">Creating New Team</h1>

<br>

<div class="sidedar large-4 columns left">&nbsp;</div>

<div class="large-4 columns">

	<%= form_for @team do |f|%>
		<%= f.label :name %>
		<%= f.text_field :name %>
		<br>
		<%= f.label :description %>
		<%= f.text_field :description %>
		<br>

		<%= f.submit 'Save' %>
		<%= link_to 'Cancel', teams_path %>
	<% end %>

</div>
